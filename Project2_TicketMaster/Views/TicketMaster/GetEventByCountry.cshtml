
@{
    ViewData["Title"] = "GetEventByCountry";
}

<h1>Get Events By Country</h1>

<form action="../TicketMaster/GetEventByCountry" method="post">
    @Html.DropDownList("country", new SelectList(Enum.GetValues(typeof(Country))),
        "Country", new { @class = "form-control" })
    <input type="submit" name="name" value="Select Country" />
</form>

